{
  "name": "vk-desktop",
  "version": "0.3.3",
  "description": "Мессенджер ВКонтакте",
  "main": "app/index.js",
  "scripts": {
    "dev": "vite",
    "build": "yarn run dist && yarn run pack",
    "lint:eslint": "eslint src main-process scripts .eslintrc.js .eslintrc.prepush.js vite.config.ts --ext .js,.ts,.tsx,.mjs",
    "lint:stylelint": "stylelint src/**/*.css",
    "lint": "yarn lint:eslint && yarn lint:stylelint",
    "generate-schemes": "node ./scripts/generateSchemes.mjs",
    "dist": "vite build",
    "pack": "electron-builder --config build/config.json",
    "win-setup": "yarn win-setup64 && yarn win-setup32",
    "win-setup64": "innosetup-compiler build/config-x64.iss --verbose",
    "win-setup32": "innosetup-compiler build/config-ia32.iss --verbose",
    "postinstall": "husky install"
  },
  "repository": "https://github.com/danyadev/vk-desktop",
  "author": "danyadev <danyadev@mail.ru>",
  "license": "Apache-2.0",
  "engines": {
    "node": "^14.17.0 || >=16.0.0"
  },
  "lint-staged": {
    "*.{js,ts,tsx,mjs}": "eslint -c .eslintrc.prepush.js",
    "*.css": "stylelint"
  },
  "dependencies": {
    "@electron/remote": "2.0.1",
    "lottie-web": "5.9.6",
    "vue": "3.2.39",
    "vue-router": "4.1.5"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.38.0",
    "@typescript-eslint/parser": "5.38.0",
    "@vitejs/plugin-vue-jsx": "2.0.1",
    "@vkontakte/appearance": "https://github.com/VKCOM/Appearance#9.51.3",
    "electron": "12.2.3",
    "electron-builder": "23.5.1",
    "eslint": "8.22.0",
    "eslint-import-resolver-typescript": "3.5.1",
    "eslint-plugin-import": "npm:eslint-plugin-i",
    "husky": "8.0.1",
    "innosetup-compiler": "6.2.0",
    "lint-staged": "13.0.3",
    "postcss-html": "1.5.0",
    "stylelint": "14.12.1",
    "stylelint-config-standard": "28.0.0",
    "typescript": "4.8.3",
    "vite": "3.1.3",
    "vite-plugin-electron": "0.9.3",
    "vite-svg-loader": "3.6.0"
  }
}
