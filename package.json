{
  "name": "vk-desktop",
  "version": "0.3.0-alpha.4",
  "description": "Клиент ВКонтакте",
  "scripts": {
    "dev": "webpack-dev-server --mode development --hot",
    "build": "yarn run dist && yarn run pack",
    "lint": "eslint src --ext .vue,.ts,.js && stylelint src/**/*.{vue,css}",
    "test": "jest",
    "dist": "webpack --mode production",
    "pack": "electron-builder --config build/config.json",
    "win-setup": "yarn win-setup64 && yarn win-setup32",
    "win-setup64": "innosetup-compiler build/config-x64.iss --verbose",
    "win-setup32": "innosetup-compiler build/config-ia32.iss --verbose"
  },
  "repository": "https://github.com/danyadev/vk-desktop",
  "author": "danyadev <danyadev@mail.ru>",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/jest": "26.0.8",
    "@typescript-eslint/eslint-plugin": "3.7.1",
    "@typescript-eslint/parser": "3.7.1",
    "@vue/compiler-dom": "3.0.0-rc.5",
    "@vue/compiler-sfc": "3.0.0-rc.5",
    "copy-webpack-plugin": "6.0.3",
    "css-loader": "4.2.0",
    "electron": "8.4.1",
    "electron-builder": "22.8.0",
    "eslint": "7.6.0",
    "eslint-import-resolver-webpack": "0.12.2",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-vue": "7.0.0-beta.1",
    "file-loader": "6.0.0",
    "innosetup-compiler": "5.6.1",
    "jest": "26.2.2",
    "lottie-web": "5.7.1",
    "mini-css-extract-plugin": "0.9.0",
    "stylelint": "13.6.1",
    "stylelint-config-standard": "20.0.0",
    "terser-webpack-plugin": "3.0.8",
    "ts-jest": "26.1.4",
    "ts-loader": "8.0.2",
    "typescript": "3.9.7",
    "vue": "3.0.0-rc.5",
    "vue-eslint-parser": "7.1.0",
    "vue-loader": "16.0.0-beta.4",
    "vue-router": "4.0.0-beta.4",
    "vue-svg-loader": "danyadev/vue-svg-loader#next",
    "vuex": "4.0.0-beta.4",
    "webpack": "4.44.1",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0"
  }
}
